{
    "version": 8,
    "name": "SwissMap style",
    "sources": {
      "swissmap": {
        "type": "vector",
        "url": "https://wipfli.github.io/swiss-map/tilejson-swissmap.json"
      },
      "qrank": {
        "type": "vector",
        "url": "https://wipfli.github.io/swiss-map/tilejson-qrank.json"
      },
      "landcover": {
        "type": "vector",
        "url": "https://wipfli.github.io/swiss-map/tilejson-h3-landcover.json"
      },
      "swisstopo-vector-hillshade": {
        "type": "vector",
        "url": "https://wipfli.github.io/swiss-map/tilejson-swisstopo-vector-hillshade.json",
        "attribution": "<a href=\"https://geo.admin.ch\">Â© swisstopo</a>"
      },
      "bikeroutes": {
        "type": "vector",
        "tiles": [
          "https://tiles.swiss-map.cc/bikeroutes/{z}/{x}/{y}.pbf"
        ],
        "maxzoom": 14
      },
      "protomaps": {
        "type": "vector",
        "tiles": [
          "https://tiles.swiss-map.cc/protomaps/{z}/{x}/{y}.pbf"
        ],
        "minzoom": 12,
        "maxzoom": 15
      }
    },
    "light": {
      "intensity": 0.1
    },
    "glyphs": "https://wipfli.github.io/swiss-map/font/{fontstack}/{range}.pbf",
    "sprite":"https://maputnik.github.io/osm-liberty/sprites/osm-liberty",
    "layers": [
      {
        "id": "background",
        "type": "background",
        "layout": {
          "visibility": "visible"
        },
        "paint": {
          "background-color": "#fff"
        }
      },
      
      {
        "id": "waterway",
        "type": "line",
        "source": "swissmap",
        "source-layer": "waterway",
        "layout": {
          "visibility": "visible"
        },
        "paint": {
          "line-color": "#abcdfb",
          "line-width": {
            "stops": [
              [
                9,
                1
              ],
              [
                11,
                2
              ]
            ]
          }
        }
      },
      {
        "id": "water",
        "type": "fill",
        "source": "swissmap",
        "source-layer": "water",
        "layout": {
          "visibility": "visible"
        },
        "paint": {
          "fill-color": "#abcdfb"
        }
      },
      {
        "id": "bikeroutes",
        "type": "line",
        "source": "bikeroutes",
        "source-layer": "bikeroutes",
        "minzoom": 10,
        "layout": {
          "visibility": "visible",
          "line-cap": "round",
          "line-join": "round"
        },
        "paint": {
          "line-color": "#61c455",
          "line-opacity": 1.0,
          "line-width": [
            "interpolate",
            ["linear"],
            ["zoom"],
            10, 4,
            14, 12,
            20, 30
          ]
        }
      },
      {
        "id": "highway-path",
        "type": "line",
        "source": "swissmap",
        "source-layer": "highway-path",
        "layout": {
          "line-cap": "round",
          "line-join": "round",
          "visibility": "visible"
        },
        "paint": {
          "line-color": "#000",
          "line-dasharray": [
            0.0,
            2.0
          ],
          "line-width": {
            "stops": [
              [
                12,
                1
              ],
              [
                14,
                2
              ],
              [
                20,
                3
              ]
            ]
          }
        }
      },
      {
        "id": "highway-footway",
        "type": "line",
        "source": "swissmap",
        "source-layer": "highway-footway",
        "layout": {
          "line-cap": "round",
          "line-join": "round",
          "visibility": "visible"
        },
        "paint": {
          "line-color": "#000",
          "line-dasharray": [
            0.0,
            2.0
          ],
          "line-width": {
            "stops": [
              [
                14,
                2
              ],
              [
                20,
                3
              ]
            ]
          }
        }
      },
      {
        "id": "highway-tracktype-2",
        "type": "line",
        "source": "swissmap",
        "source-layer": "highway-tracktype-2",
        "layout": {
          "visibility": "visible"
        },
        "paint": {
          "line-color": "#000",
          "line-width": {
            "stops": [
              [
                12,
                1
              ],
              [
                14,
                1.5
              ]
            ]
          }
        }
      },
      {
        "id": "highway-tracktype-3-4-5",
        "type": "line",
        "source": "swissmap",
        "source-layer": "highway-tracktype-3-4-5",
        "layout": {
          "visibility": "visible"
        },
        "paint": {
          "line-color": "#000",
          "line-dasharray": [
            10.0,
            2.0
          ],
          "line-width": {
            "stops": [
              [
                12,
                1
              ],
              [
                14,
                1.5
              ],
              [
                20,
                3
              ]
            ]
          }
        }
      },
      {
        "id": "highway",
        "type": "line",
        "source": "swissmap",
        "source-layer": "highway",
        "layout": {
          "line-cap": "round",
          "line-join": "round",
          "visibility": "visible",
          "line-sort-key": [
            "to-number",
            [
              "get",
              "line-sort-key"
            ]
          ]
        },
        "paint": {
          "line-color": [
            "case",
            ["==", ["get", "is-casing"], "no"],
            "#000",
            "#fff"
          ],
          "line-width": [
            "interpolate",
            [
              "linear"
            ],
            [
              "zoom"
            ],
            14,
            [
              "case",
              [
                "all",
                [
                  "<",
                  [
                    "to-number",
                    [
                      "get",
                      "line-width"
                    ]
                  ],
                  2
                ],
                ["==", ["get", "is-casing"], "no"]
              ],
              2,
              [
                "all",
                [
                  "<",
                  [
                    "to-number",
                    [
                      "get",
                      "line-width"
                    ]
                  ],
                  3
                ],
                ["==", ["get", "is-casing"], "yes"]
              ],
              3,
              [
                "to-number",
                [
                  "get",
                  "line-width"
                ]
              ]
            ],
            20,
            [
              "to-number",
              [
                "get",
                "line-width-z20"
              ]
            ]
          ]
        }
      },
      
      {
        "id": "labels",
        "type": "symbol",
        "source": "qrank",
        "source-layer": "qrank",
        "layout": {
          "text-field": [
            "case",
            [
              "==",
              [
                "get",
                "place"
              ],
              "country"
            ],
            [
              "get",
              "name:en"
            ],
            [
              "==",
              [
                "get",
                "place"
              ],
              "sea"
            ],
            [
              "get",
              "name:en"
            ],
            [
              "==",
              [
                "get",
                "place"
              ],
              "ocean"
            ],
            [
              "get",
              "name:en"
            ],
            [
              "has",
              "ele"
            ],
            ["format", ["get", "name"], {}, "\n", {}, ["get", "ele"], {"font-scale": 0.8}, "m", {"font-scale": 0.8}],
            [
              "get",
              "name"
            ]
          ],
          "text-size": [
            "interpolate",
            [
              "linear"
            ],
            [
              "zoom"
            ],
            7,
            [
              "*",
              1.1,
              [
                "case",
                [
                  ">",
                  [
                    "to-number",
                    [
                      "get",
                      "@qrank"
                    ]
                  ],
                  10000000
                ],
                17,
                [
                  ">",
                  [
                    "to-number",
                    [
                      "get",
                      "@qrank"
                    ]
                  ],
                  1000000
                ],
                16,
                [
                  ">",
                  [
                    "to-number",
                    [
                      "get",
                      "@qrank"
                    ]
                  ],
                  100000
                ],
                14,
                [
                  ">",
                  [
                    "to-number",
                    [
                      "get",
                      "@qrank"
                    ]
                  ],
                  10000
                ],
                12,
                11
              ]
            ],
            12,
            [
              "*",
              1.5,
              [
                "case",
                [
                  ">",
                  [
                    "to-number",
                    [
                      "get",
                      "@qrank"
                    ]
                  ],
                  10000000
                ],
                17,
                [
                  ">",
                  [
                    "to-number",
                    [
                      "get",
                      "@qrank"
                    ]
                  ],
                  1000000
                ],
                16,
                [
                  ">",
                  [
                    "to-number",
                    [
                      "get",
                      "@qrank"
                    ]
                  ],
                  100000
                ],
                14,
                [
                  ">",
                  [
                    "to-number",
                    [
                      "get",
                      "@qrank"
                    ]
                  ],
                  10000
                ],
                12,
                11
              ]
            ]
          ],
          "text-font": [
            "Fira Sans Regular"
          ],
          "visibility": "visible",
          "symbol-sort-key": [
            "-",
            [
              "to-number",
              [
                "get",
                "@qrank"
              ]
            ]
          ]
        },
        "paint": {
          "text-color": [
            "case",
            [
              "==",
              [
                "get",
                "natural"
              ],
              "water"
            ],
            "#000",
            [
              "==",
              [
                "get",
                "natural"
              ],
              "bay"
            ],
            "#000",
            [
              "==",
              [
                "get",
                "place"
              ],
              "sea"
            ],
            "#000",
            [
              "==",
              [
                "get",
                "place"
              ],
              "ocean"
            ],
            "#000",
            [
              "==",
              [
                "get",
                "place"
              ],
              "state"
            ],
            "purple",
            [
              "has",
              "place"
            ],
            "black",
            "gray"
          ],
          "text-halo-color": "#fff",
          "text-halo-width": 3
          
          
        }
      }
    ],
    "id": "swissmap-style-mittag"
  }